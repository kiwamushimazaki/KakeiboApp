@Model IEnumerable<MvcBasic.Models.Income>

    @{
        ViewBag.Title = "IncomeList";
    }

    <h2>IncomeList</h2>
    <table class="table">
        <tr>
            <th>用途</th>
            <th>ユーザ</th>
            <th>日付</th>
            <th>値段</th>
            <th>備考</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.UserName</td>
                <td>@item.Date</td>
                <td>@item.Price</td>
                <td>@item.Memo</td>
            </tr>
        }
    </table>

